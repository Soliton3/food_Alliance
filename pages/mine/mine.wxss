/* pages/mine/mine.wxss */
.head{
  background-color: #f60;
  height: 400rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.head image{
  width:200rpx;
  height: 200rpx;
  border-radius: 50%;
}

.cell{
  font-size: 16px;
  margin: 35rpx 0rpx;
  border-top: 2rpx solid #ddd;
}
.cell-item{
  border-bottom: 1rpx solid #ddd;
  padding: 20rpx 30rpx;
}
.cell-te {
  border-bottom: 1rpx solid #ddd;
  padding: 20rpx 30rpx;
  display: flex;
  flex-direction: row;
}
.cell-te .right{
  margin-left: 290rpx;
  color: gray;
}
.uinfo{
  display: flex;
  flex-direction: column;
  align-items: center;
  color:whitesmoke;
}